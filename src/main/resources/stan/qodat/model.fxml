<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import stan.qodat.scene.control.ViewNodeListView?>
<VBox styleClass="viewer-box" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="stan.qodat.scene.controller.ModelController">
  <children>
      <ViewNodeListView id="models-list" fx:id="modelListView" prefWidth="220.0">
          <contextMenu>
              <ContextMenu>
                  <items>
                      <MenuItem fx:id="setLabel" mnemonicParsing="false" text="Set label" />
                  </items>
              </ContextMenu>
          </contextMenu>
      </ViewNodeListView>
      <TextField fx:id="searchModelField" prefHeight="27.0" prefWidth="220.0" promptText="Search model" />
  </children>
</VBox>
