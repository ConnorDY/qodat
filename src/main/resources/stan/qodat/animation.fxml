<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox styleClass="viewer-box" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="stan.qodat.scene.controller.AnimationController"
      stylesheets="@style.css">
    <children>
        <ListView styleClass="animations-list" fx:id="animationsListView"/>
        <TextField fx:id="searchTextField" maxHeight="27.0" promptText="Search animation"/>
    </children>
</VBox>
